javascript: (function (e) {
    let boothlet = "https://iranika.info/boothlet/";
    let target = window.open(boothlet, null, 'width=520,height=500');
    let pd = JSON.parse(localStorage.getItem("nikalib") ?? `[['', 'localStorageのキーがみつかりませんでした',"",""]]`);
    setTimeout(() => {
        target.postMessage({ pd: pd }, boothlet);
    }, 1000);
    console.log(target, pd);
})();